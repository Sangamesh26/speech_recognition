   "source": [
    "import speech_recognition as sr\n",
    "rec=sr.Recognizer()\n",
    "with sr.Microphone() as source:\n",
    "    print(\"Audio please :: \")\n",
    "    audio=rec.listen(source)\n",
    "    try:\n",
    "        text=rec.recognize_google(audio)\n",
    "        print(\"Your audio was :: \", text)\n",
    "    except:\n",
    "        print(\"Not recognized\")"
   ]
